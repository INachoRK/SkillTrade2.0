package co.edu.uco.skilltrade.dto;

import java.util.UUID;

public final class CursoDTO {
	
	private UUID id;
	private String titulo;
	private String descripcion;
	private String categoria;
	
	public CursoDTO(UUID id, String titulo, String descripcion, String categoria) {
		sqqqqqqqqqqqq
		this.id = id;
		this.titulo = titulo;
		this.descripcion = descripcion;
		this.categoria = categoria;
	}
	
	private final UUID setId(UUID id) {
		return this.id = id;
	}
	
	private final String setTitulo(String titulo) {
		return this.titulo = titulo;
	}
	
	private final String setDescripcion(String descripcion) {
		return this.descripcion = descripcion;
	}
	
	private final String setCategoria(String categoria) {
		return this.categoria = categoria;
	}
	
	public final UUID getId() {
		return id;
	}
	
	public final String getTitulo() {
		return titulo;
	}
	
	public final String getDescripcion() {
		return descripcion;
	}
	
	public final String getCategoria() {
		return categoria;
	}
	
}

