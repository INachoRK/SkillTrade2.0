package co.edu.uco.skilltrade.business.domain;

public class CursoDomain {

	private int id;
	private String titulo;
	private String descripcion;
	private String categoria;
	
	private CursoDomain(int id, String nombre, DepartamentoDomain departamento) {
		setId(id);
		setNombre(nombre);
		setDepartamento(departamento);
	}

	private void setId(int id) {
		this.id = id;
	}

	private void setNombre(String nombre) {
		this.nombre = nombre;
	}

	private void setDepartamento(DepartamentoDomain departamento) {
		this.departamento = departamento;
	}

	public int getId() {
		return id;
	}

	public String getNombre() {
		return nombre;
	}

	public DepartamentoDomain getDepartamento() {
		return departamento;
	}

	
	
}
